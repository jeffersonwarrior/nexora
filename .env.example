# Nexora v0.29.1-RC1 - Synthetic Keys Setup Guide
# =============================================

# Copy this file to /home/nexora/.env and fill in your synthetic keys
# Then run: source /home/nexora/.env

# ==============================================================================
# REQUIRED: For VCR Recording (Agent Integration Tests)
# ==============================================================================

# Anthropic (Claude Sonnet 4.5 + Claude Haiku)
NEXORA_ANTHROPIC_API_KEY=your_synthetic_anthropic_key_here

# OpenAI (GPT-5 + GPT-4o)
NEXORA_OPENAI_API_KEY=your_synthetic_openai_key_here

# OpenRouter (Kimi K2 + Qwen)
NEXORA_OPENROUTER_API_KEY=your_synthetic_openrouter_key_here

# ZAI (GLM-4.6 + GLM-4.5-Air)
NEXORA_ZAI_API_KEY=your_synthetic_zai_key_here

# ==============================================================================
# OPTIONAL: For Other Provider Tests
# ==============================================================================

# Google Gemini
NEXORA_GOOGLE_API_KEY=

# AWS Bedrock
NEXORA_BEDROCK_ACCESS_KEY=
NEXORA_BEDROCK_SECRET_KEY=
NEXORA_BEDROCK_REGION=us-east-1

# ==============================================================================
# OTHER TEST ENV VARS (Optional)
# ==============================================================================

# For memory tests
NEXORA_MEMORY_PATH=/tmp/nexora-test-memory

# For shell tests (if needed)
NEXORA_SHELL_TEST_MODE=true

# For TUI tests (if running via tmux)
NEXORA_TUI_TEST_MODE=true
